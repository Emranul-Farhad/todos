{
    "version": 2,

    "builds": [
       { "src": "index.js", 
       "use": "@vercel/node"
     }
    ],
    "routes": [
       {
        
        "src": "/(.*)",
         "GET": "/story" ,
         "POST" : "/send",
         "PUT": "/post/:id",
         "DELETE" :  "/delete/:id",
        "dest": "/index.js",
        "methods" : [ "GET", "POST", "PUT", "DELETE"]
    
    }
    ]
 }